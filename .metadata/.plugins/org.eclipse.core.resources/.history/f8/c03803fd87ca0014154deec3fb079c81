	private RnConsultarSituacao() {}
	
	private static RnConsultarSituacao aRnConsultarSituacao = new RnConsultarSituacao();
	
	public static RnConsultarSituacao getInstance(){
		if(RnConsultarSituacao.aRnConsultarSituacao == null){
			RnConsultarSituacao.aRnConsultarSituacao = new RnConsultarSituacao();
		}
		
		return RnConsultarSituacao.aRnConsultarSituacao;
	}
	
	public void executar(){
		
	}

}
